{
  "manifest_version": 3,
  "name": "uwu-chan",
  "version": "1.2.0",
  "description": "i will make a webbusite become uwu",

  "permissions": ["storage"],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/img/icon16.png",
      "32": "/img/icon32.png",
      "64": "/img/icon64.png",
      "128": "/img/icon128.png"
    }
  },
  "background": {
    "service_worker": "./javascript/background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./javascript/uwu.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["./javascript/uwutube.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "/img/icon16.png",
    "32": "/img/icon32.png",
    "64": "/img/icon64.png",
    "128": "/img/icon128.png"
  },
  "options_page": "options.html"
}
